#include <bits/stdc++.h>
using namespace std;

int main()
{
    string str;
    cout<<"\n Enter a string : ";
    cin>>str;
    cout<<"\n String : "<<str;
    
    string str1="I Love Myself";
    str1.push_back('.');
    cout<<"\n After push back function : "<<str1;
    str1.pop_back();
    cout<<"\n After pop back function : "<<str1;

    return 0;
}
